{
  "name": "@atweel/graphql-auth-transformer",
  "version": "7.0.0-beta.0",
  "description": "Implements the @auth directive for the appsync model transform.",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-cli.git",
    "directory": "packages/graphql-auth-transformer"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci -i",
    "build": "tsc",
    "clean": "rimraf ./lib",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@atweel/graphql-connection-transformer": "4.18.0-beta.0",
    "@atweel/graphql-mapping-template": "4.14.0-beta.0",
    "@atweel/graphql-transformer-common": "4.16.0-beta.0",
    "@atweel/graphql-transformer-core": "7.0.0-beta.0",
    "graphql": "^14.5.8"
  },
  "devDependencies": {
    "@atweel/graphql-dynamodb-transformer": "7.0.0-beta.0",
    "@atweel/graphql-elasticsearch-transformer": "4.8.0-beta.0",
    "@atweel/graphql-function-transformer": "2.4.0-beta.0",
    "@types/node": "^10.17.13",
    "cloudform-types": "^4.2.0",
    "rimraf": "^3.0.0"
  },
  "jest": {
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(src/__tests__/.*.test.ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true
  },
  "publishConfig": {
    "access": "restricted"
  }
}
