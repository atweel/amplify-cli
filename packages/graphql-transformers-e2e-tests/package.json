{
  "name": "@atweel/graphql-transformers-e2e-tests",
  "version": "7.0.0-beta.1",
  "description": "End to end functional tests for appsync supported transformers.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-cli.git",
    "directory": "packages/graphql-transformers-e2e-tests"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "scripts": {
    "e2e": "jest",
    "build-tests": "tsc --build tsconfig.tests.json"
  },
  "dependencies": {
    "@atweel/graphql-transformer-common": "4.16.0-beta.0",
    "@atweel/graphql-transformer-core": "7.0.0-beta.0",
    "amazon-cognito-identity-js": "^3.2.2",
    "axios": "^0.19.2",
    "cloudform-types": "^4.2.0",
    "graphql": "^14.5.8",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@atweel/graphql-auth-transformer": "7.0.0-beta.0",
    "@atweel/graphql-connection-transformer": "4.18.0-beta.0",
    "@atweel/graphql-dynamodb-transformer": "7.0.0-beta.0",
    "@atweel/graphql-elasticsearch-transformer": "4.8.0-beta.0",
    "@atweel/graphql-function-transformer": "2.4.0-beta.0",
    "@atweel/graphql-http-transformer": "4.16.0-beta.0",
    "@atweel/graphql-key-transformer": "2.18.0-beta.0",
    "@atweel/graphql-predictions-transformer": "2.3.6-beta.0",
    "@atweel/graphql-versioned-transformer": "4.16.0-beta.0",
    "@aws-amplify/core": "^2.1.0",
    "@types/node": "^10.17.13",
    "aws-amplify": "^2.2.2",
    "aws-appsync": "^2.0.2",
    "aws-cdk": "^1.32.2",
    "aws-sdk": "^2.608.0",
    "execa": "4.0.0",
    "fs-extra": "^8.1.0",
    "graphql-tag": "^2.10.1",
    "isomorphic-fetch": "^2.2.1",
    "jest-junit": "^10.0.0",
    "node-fetch": "^2.6.0",
    "ws": "^7.2.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/__tests__/**",
      "!**/*.d.ts"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*.test.*)$",
    "testPathIgnorePatterns": [
      "/lib/",
      "/resources/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "window": {},
      "ts-jest": {
        "tsConfig": "tsconfig.tests.json"
      }
    }
  },
  "jest-junit": {
    "outputDirectory": "reports/junit/",
    "outputName": "js-test-results.xml",
    "usePathForSuiteName": "true",
    "addFileAttribute": "true"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
